<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:id>VPDC@columbia.edu</em:id>
    <em:name>Validating Programmatic DOM Changes</em:name>
    <em:description>
      This extensions helps developers find problems with the dynamic HTML content in the DOM tree.
      1. It shows which HTML elements have not detached their handlers before destroying them.
      2. It shows the invalid HTML with respect to the DOCTYPE used.
    </em:description>
    <em:version>0.1</em:version>
    <em:creator>Aharon Mitchell (UNI) and Lukarov Peter (pbl2108)</em:creator>
    <em:homepageURL>http://www.columbia.edu</em:homepageURL>
    <em:type>2</em:type>

    <!-- Mozilla Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>4.0</em:minVersion>
        <em:maxVersion>10.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
